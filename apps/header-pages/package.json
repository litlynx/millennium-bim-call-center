{
  "name": "header-pages",
  "version": "0.1.0",
  "main": "./src/App.tsx",
  "types": "./src/App.tsx",
  "scripts": {
    "analyze": "rspack --config rspack/rspack.prod.ts --env analyze",
    "predev": "rimraf dist",
    "dev": "rspack serve --config rspack/rspack.dev.ts",
    "prebuild": "rimraf dist",
    "build": "rspack --config rspack/rspack.prod.ts",
    "biome:lint": "biome lint ./src",
    "biome:format": "biome format ./src --write",
    "biome:check": "biome check ./src",
    "biome:check:apply": "biome check ./src --write",
    "test": "bun test",
    "test:ci": "bun test --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "types:generate": "rspack --config rspack/rspack.prod.ts --env typesOnly",
    "types:clean": "rimraf .wp_federation",
    "types:build": "bun run types:clean && bun run types:generate",
    "serve:prod": "serve -s dist -l ${HEADER_PAGES_PREVIEW_PORT:-8082}"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet": "^6.1.0",
    "react-router": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@config/babel-config": "workspace:*",
    "@config/bun-test-config": "workspace:*",
    "@config/tsconfig": "workspace:*",
    "@config/rspack-config": "workspace:*",
    "@rspack/cli": "^1.5.2",
    "@rspack/core": "^1.5.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-helmet": "^6.1.11",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "dts-loader": "^0.1.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack-merge": "^6.0.1"
  }
}
