{
  "name": "sidebar-pages",
  "version": "0.1.0",
  "main": "./src/App.tsx",
  "types": "./src/App.tsx",
  "scripts": {
    "analyze": "rspack --config rspack/rspack.prod.ts --env analyze",
    "rsdoctor": "RSDOCTOR=true bun run build",
    "predev": "rimraf dist",
    "dev": "rspack serve --config rspack/rspack.dev.ts",
    "prebuild": "rimraf dist",
    "build": "rspack --config rspack/rspack.prod.ts",
    "biome:lint": "biome lint ./src",
    "biome:format": "biome format ./src --write",
    "biome:check": "biome check ./src",
    "biome:check:apply": "biome check ./src --write",
    "test": "bun test",
    "test:ci": "bun test --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "types:generate": "rspack --config rspack/rspack.prod.ts --env typesOnly",
    "types:clean": "rimraf .wp_federation",
    "types:build": "bun run types:clean && bun run types:generate",
    "serve:prod": "bunx serve -s dist -l $SIDEBAR_PAGES_PREVIEW_PORT"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet": "^6.1.0",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@config/babel-config": "workspace:*",
    "@config/bun-test-config": "workspace:*",
    "@config/tsconfig": "workspace:*",
    "@config/rspack-config": "workspace:*",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-helmet": "^6.1.11",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "dts-loader": "^0.1.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack-merge": "^6.0.1"
  }
}
